<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sateesh.com.completeui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TabbedView"
            android:label="@string/title_activity_tabbed_view"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="sateesh.com.completeui.MainActivity" />
        </activity>
        <activity
            android:name=".DoneBar"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".TabbedWithFloating"
            android:label="@string/title_activity_tabbed_with_floating"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="sateesh.com.completeui.MainActivity" />
        </activity>
    </application>

</manifest>
